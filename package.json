{
  "homepage": "https://junevarsha.github.io/pet-me/",
  "name": "pet-me",
  "version": "1.0.0",
  "description": "react-dom - allows react to be rendered to the browser react native and reactdom share the react package",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development parcel src/index.html --open",
    "format": "prettier --write \"src/**/*.{js,jsx,html}\"",
    "predeploy": "rm -rf dist && parcel build src/index.html --public-url='./'",
    "deploy": "gh-pages -d dist",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junevarsha/pet-me.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/junevarsha/pet-me/issues"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@frontendmasters/pet": "^1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
